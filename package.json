{
  "name": "compote",
  "version": "0.6.1",
  "license": "MIT",
  "scripts": {
    "src:rollup": "rollup ./build/src/compote.js -o ./build/compote.js -g mithril:m -f iife -n Compote",
    "todomvc:rollup": "rollup ./build/examples/todomvc/todomvc.js -o ./build/todomvc.js -g mithril:m -f iife -n TodoMVC",
    "build": "tsc && npm run src:rollup && npm run todomvc:rollup",

    "start": "serve",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "mithril": "1.0.1"
  },
  "devDependencies": {
    "@types/mithril": "0.0.31",
    "es6ify": "1.6.0",
    "rollup": "0.41.4",
    "serve": "3.3.1",
    "typescript": "2.1.6"
  }
}
